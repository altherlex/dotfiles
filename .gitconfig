[user]
  name = [seu_user]
  email = [seu_email]
[push]
  default = current
[alias]
  co = "checkout"
  st = "status -b"
  ls = "log --first-parent --graph --pretty=format:'%Cred%h%Creset | %cn | -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative"
  graph = "log --graph --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset'"
[color]
  ui = auto
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  status = auto
[color "status"]
  changed = yellow
